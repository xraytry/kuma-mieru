    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.5.0",
    "intl-messageformat": "^10.7.15",
    "json5": "^2.2.3",
    "lucide-react": "^0.482.0",
    "markdown-it": "^14.1.0",
    "next": "^15.2.3",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.1",
    "tailwind-merge": "^3.0.2",

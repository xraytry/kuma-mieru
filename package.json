{
  "name": "kuma-mieru",
  "description": "A 3rd-party Uptime Kuma monitoring dashboard built on Next.js 15, TypeScript and Recharts.",
  "version": "1.0.7",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3881",
    "build": "next build",
    "start": "bun .next/standalone/server.js",
    "lint": "biome check .",
    "lint:fix": "biome check --apply .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "predev": "bun run generate-image-domains && bun run banner",
    "prebuild": "bun run generate-image-domains && bun run banner",
    "prestart": "bun run banner && bun run generate-image-domains",
    "generate-image-domains": "bun scripts/generate-image-domains.ts",
    "banner": "bun scripts/banner.ts"
  },
